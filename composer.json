{
    "name": "paxha/laravel-sluggable",
    "description": "This package provides an event that will generate a unique slug when saving or creating any Eloquent model.",
    "type": "laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel package",
        "laravel slug generator",
        "slug generator",
        "sluggable",
        "laravel sluggable"
    ],
    "authors": [
        {
            "name": "Hassan Raza Pasha",
            "email": "e.hassanrazapasha@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "orchestra/testbench-dusk": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sluggable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sluggable\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
